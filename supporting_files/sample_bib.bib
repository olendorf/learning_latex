   pages = {94-102},
   title = {Gray space and green space proximity associated with higher anxiety in youth with autism.},
   volume = {53},
   url = {https://proxying.lib.ncsu.edu/index.php?url=https://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2018-47047-013&site=ehost-live&scope=site},
   year = {2018},
}
