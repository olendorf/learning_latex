   journal = {Human brain mapping},
   keywords = {Adolescent,Autism Spectrum Disorder/*diagnostic imaging,Autism Spectrum Disorder/psychology,Brain/*diagnostic imaging,Child,Cognition/physiology,Female,Humans,Magnetoencephalography*,Male,Neuropsychological Tests,alpha*,autism spectrum disorders*,magnetoencephalography*,maturation*,resting-state*},
   pages = {3288-3298},
   title = {Abnormal maturation of the resting-state peak alpha frequency in children with autism spectrum disorder.},
   volume = {40},
   url = {https://proxying.lib.ncsu.edu/index.php?url=https://search.ebscohost.com/login.aspx?direct=true&db=cmedm&AN=30977235&site=ehost-live&scope=site},
